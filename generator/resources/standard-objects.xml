<?xml version="1.0" encoding="utf-8"?>
<!-- 
This file is a code generation template for Chisimba standard objects. 
Its purpose is to allow objects to be added by including a parsecode
that references them. For example to add the language object just put
...WORKING HERE..INCOMPLETE -->
<chisimba_standard_objects>

	<object>
	  <name>objLanguage</name>
	  <comment>used for generating a translatable interface</comment>
	  <obclass>language</obclass>
	  <module>language</module>
	  <instantiate>TRUE</instantiate>
	</object>
	
	<object>
	  <name>objUser</name>
	  <comment>used to provide properties and methods of users</comment>
	  <obclass>user</obclass>
	  <module>security</module>
	  <instantiate>TRUE</instantiate>
	</object>

	<object>
	  <name>objLogger</name>
	  <comment>used to log user activity on the system</comment>
	  <obclass>logger</obclass>
	  <module>logger</module>
	  <instantiate>FALSE</instantiate>
	</object>

	<object>
	  <name>objConfig</name>
	  <comment>used to provide access to configuration properties</comment>
	  <obclass>config</obclass>
	  <module>config</module>
	  <instantiate>TRUE</instantiate>
	</object>

</chisimba_standard_objects>